{
  "kind": "collectionType",
  "collectionName": "questions",
  "info": {
    "singularName": "question",
    "pluralName": "questions",
    "displayName": "Câu hỏi",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "enabled": {
      "type": "boolean",
      "default": true
    },
    "order": {
      "type": "integer"
    },
    "isText": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "reportName": {
      "type": "string"
    },
    "survey": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::survey.survey",
      "inversedBy": "questions"
    },
    "question": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::question.question"
    },
    "level": {
      "type": "integer",
      "default": 0
    },
    "SurveyQ": {
      "type": "component",
      "repeatable": true,
      "component": "survey-question.survey-question"
    },
    "inline": {
      "type": "boolean",
      "default": false
    },
    "isMultiChoice": {
      "type": "boolean",
      "default": false
    },
    "appendices": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::appendix.appendix",
      "mappedBy": "question"
    }
  }
}
